   "source": [
    "#all uniqie subsets in a set\n",
    "arr=[1,2,2,3]\n",
    "n=len(arr)\n",
    "s=[]\n",
    "for i in range(n):\n",
    "    for j in range(i,n+1):\n",
    "        u=set()\n",
    "        for k in range(i,j):\n",
    "            u.add(arr[k])\n",
    "        if list(u) not in s:\n",
    "            s.append(list(u))\n",
    "print(s)"
   ]
  }
